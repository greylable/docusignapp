<div class="row">
  <div class="col s8 offset-s2">
    <div class="card">
      <div class="card-content">
        <span class="card-title">List an envelope to create</span>
        <%= simple_form_for @newenvelope do |f| %>
          <%= f.input :name%>
          <%= f.input :rental%>
          <%= f.input :nric%>
          <%= f.input :email%>
          <div class="card-action">
            <%= link_to 'back', :back %>
            <%= f.button :submit, class: 'right' %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>